            <RowDefinition Height="235*"/>
            <RowDefinition Height="237*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Label x:Name="label4" Content="Cadastro" Height="34" Margin="152,62,152,0" VerticalAlignment="Top" FontSize="14.667" Grid.ColumnSpan="2"/>
        <Label x:Name="label5" Content="Consulta" Grid.Column="1" Height="34" Margin="187.2,62,0,0" VerticalAlignment="Top" FontSize="14.667" HorizontalAlignment="Left" Width="68"/>
        <Button x:Name="btnCadUsu" Grid.ColumnSpan="2" Content="CADASTRAR USUÁRIO" Margin="60,140,241.4,54.2" Click="btnCadUsu_Click"/>
        <Button x:Name="btnCadFor" Grid.ColumnSpan="2" Content="CADASTRAR FORNECEDOR" Height="40" Margin="60,210,241.4,0" VerticalAlignment="Top" Click="btnCadFor_Click" Grid.RowSpan="2"/>
