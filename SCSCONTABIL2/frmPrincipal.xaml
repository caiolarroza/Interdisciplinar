        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SCSCONTABIL2"
        mc:Ignorable="d"
        Title="Principal" Height="500" Width="1128.788" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
