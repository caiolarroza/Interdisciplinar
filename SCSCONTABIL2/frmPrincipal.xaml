        <Button x:Name="btnSair" Content="SAIR" Grid.Column="2" Height="51" Margin="10,130,12,0" Grid.Row="1" VerticalAlignment="Top" Click="btnSair_Click"/>
        <Button x:Name="btnLogout" Content="LOGOUT" Height="51" Margin="11,130,10,0" Grid.Row="1" VerticalAlignment="Top" Click="btnLogout_Click" Grid.Column="1"/>
        <Label x:Name="label5_Copy" Content="Venda" Grid.Column="2" Height="34" Margin="249.2,62,0,0" VerticalAlignment="Top" FontSize="14.667" HorizontalAlignment="Left" Width="68" Grid.ColumnSpan="2"/>
