        <Button x:Name="btnSair" Content="SAIR" Grid.Column="2" Height="51" Margin="10,129.8,9.6,0" Grid.Row="1" VerticalAlignment="Top" Click="btnSair_Click"/>
        <Button x:Name="btnLogout" Content="LOGOUT" Grid.Column="1" Height="51" Margin="10.4,129.8,10,0" Grid.Row="1" VerticalAlignment="Top" Click="btnLogout_Click"/>
        <Label x:Name="label5_Copy" Content="Venda" Grid.Column="2" Height="34" Margin="251.6,62,0,0" VerticalAlignment="Top" FontSize="14.667" HorizontalAlignment="Left" Width="68" Grid.ColumnSpan="2"/>
        <Button x:Name="btnvenPEPS" Content="VENDER PEPS" Margin="158.6,140,0,54.2" Grid.Column="2" HorizontalAlignment="Left" Width="236" Grid.ColumnSpan="2" Click="btnvenPEPS_Click"/>
        <Button x:Name="btnVenUEPS" Content="VENDER UEPS" Height="40" Margin="158.6,210,0,0" VerticalAlignment="Top" Grid.Column="2" Grid.RowSpan="2" HorizontalAlignment="Left" Width="236" Grid.ColumnSpan="2" Click="btnVenUEPS_Click"/>

    </Grid>
