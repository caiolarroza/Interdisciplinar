        <Button x:Name="btnCadUsu" Content="CADASTRAR USUÁRIO" Margin="60,140,237,55" Click="btnCadUsu_Click" Grid.ColumnSpan="2" Width="259" Height="40" HorizontalAlignment="Left" VerticalAlignment="Top"/>
